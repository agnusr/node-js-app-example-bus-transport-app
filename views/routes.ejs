<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Transportation Details</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <div class="container">
    <h1>Welcome, <%= name %>!</h1>
    <h2>Kerala Transportation Details</h2>
    <table>
      <thead>
        <tr>
          <th>From</th>
          <th>To</th>
          <th>Time</th>
          <th>Fare</th>
        </tr>
      </thead>
      <tbody>
        <% transportationData.forEach(route => { %>
          <tr>
            <td><%= route.from %></td>
            <td><%= route.to %></td>
            <td><%= route.time %></td>
            <td><%= route.fare %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</body>
</html>
